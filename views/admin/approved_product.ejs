<%-include('navbar.ejs')%>


<div class="container-fluid">
    <div class="row bg-white pt-3 pb-2 ">
        <div class="col-md-12 subname">
            <h3>Approved Product </h3>
        </div>
    </div>

    <div class="row bg-white mt-4">

        <div class="col-md-12 mt-3">

            <table class="table table-bordered table-sm ">
                <tr>
                    <th>Action</th>
                    <th>Sr.No</th>
                    <th>Product</th>
                    <th>Price</th>
                    <th>Business</th>
                    <th>Category</th>
                    <th>Status</th>
                </tr>

                <% for(var i=0; i<products.length;i++)  {   %>

                    <tr>
                        <td>
                           
                            <a href="/admin/review_product/<%=products[i].product_id%>">

                                <button class="btn btn-primary p-0 ps-2 pe-2 "><i class="fa fa-eye"></i></button>
                            </a>

                        </td>
                        <td><%=i+1%></td>
                        <td><%=products[i].product_name%></td>
                        <td>â‚¹&nbsp;<%=products[i].product_price%></td>
                        <td><%=products[i].product_category_name%></td>
                        <td><%=products[i].business_name%></td>
                        <td style="color: green; font-weight: bold; text-transform: uppercase; "><%=products[i].status%></td>

                    </tr>

                  <% }  %> 

            </table>


        </div>
    </div>


</div>











