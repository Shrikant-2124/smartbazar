<form action="/save_account" method="post">
<div class="container">
    <div class="row justify-content-center">
        <div class="col-md-6">
            <div class="shadow border p-3 ">
                <h3 class="text-center p-3">Create Account</h3>              
                 
                <input type="text" name="user_name" id="" class="form-control " placeholder="Full Name"><br>
                
                <input type="number" name="user_mobile" id="" class="form-control"placeholder="Mobile Number" ><br>
                
                <input type="email" name="user_email" id="" class="form-control" placeholder="Email ID"><br>
               
                <input type="password" name="user_password" id="" class="form-control" placeholder="Password"><br>
                <center><button class="btn btn-primary ">Sign Up</button></center>
            </div>
        </div>
    </div>
</div>
</form>
